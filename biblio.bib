@article{Linde:1993cn,
      author         = "Linde, Andrei D.",
      title          = "{Hybrid inflation}",
      journal        = "Phys.Rev.",
      volume         = "D49",
      pages          = "748-754",
      doi            = "10.1103/PhysRevD.49.748",
